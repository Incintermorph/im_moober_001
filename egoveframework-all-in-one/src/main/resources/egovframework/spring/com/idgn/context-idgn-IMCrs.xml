<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">


	
    <!-- 과정   -->
    <bean name="imCrsIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCrsIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_CRS"/>
    </bean>
    <bean name="imCrsIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CRSOP" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    
	
    <!-- 과정 신청자  -->
    <bean name="imCrsAplcntIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCrsAplcntIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_CRS_APLCNT"/>
    </bean>
    <bean name="imCrsAplcntIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="APL" />
        <property name="cipers"   value="17" />
        <property name="fillChar" value="0" />
    </bean>
    
	
    <!-- 과정 계획  -->
    <bean name="imCrsPlanIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCrsPlanIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_CRS_PLAN"/>
    </bean>
    <bean name="imCrsPlanIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CRSPL" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    

    <!-- 과정  마스터 -->
    <bean name="imCrsMstrIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCrsMstrIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_CRS_MSTR"/>
    </bean>
    <bean name="imCrsMstrIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CRSMT" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    
     <!-- 양성기관 -->
    <bean name="imAgncyIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imAgncyIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_AGNCY"/>
    </bean>
    <bean name="imAgncyIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CRAGN" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    
    
    <bean name="imCrsAplyTrnsfIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCrsAplyTrnsfIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_AGNCY"/>
    </bean>
    <bean name="imCrsAplyTrnsfIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CRMIG" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    
    
</beans>