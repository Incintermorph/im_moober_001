<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.0.xsd">


	
    <!-- 파일   -->
    <bean name="imCmmnFileIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCmmnFileIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_CMMN_FILE"/>
    </bean>
    <bean name="imCmmnFileIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CMNFL" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    
	
	
    <!-- 파일   -->
    <bean name="imCmmnAccssHstryIdGnrService" class="egovframework.rte.fdl.idgnr.impl.EgovTableIdGnrServiceImpl" destroy-method="destroy">
        <property name="dataSource" ref="egov.dataSource" />
        <property name="strategy"   ref="imCmmnAccssHstryIdStrategy" />
        <property name="blockSize"  value="10"/>
        <property name="table"      value="COMTECOPSEQ"/>
        <property name="tableName"  value="IM_CMMN_ACCSS_HSTRY"/>
    </bean>
    <bean name="imCmmnAccssHstryIdStrategy" class="egovframework.rte.fdl.idgnr.impl.strategy.EgovIdGnrStrategyImpl">
        <property name="prefix"   value="CMHSY" />
        <property name="cipers"   value="15" />
        <property name="fillChar" value="0" />
    </bean>
    
	
    
    
</beans>