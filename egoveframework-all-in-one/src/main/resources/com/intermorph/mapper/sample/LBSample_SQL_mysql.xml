<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.intermorph.cmmn.sample.service.SampleMapper">

    <resultMap id="detailResultMap" type="com.intermorph.cmmn.sample.vo.rs.LBSampleResultSet">
        <result column="nameCard.searchCnd"           property="nameCard.searchCnd"          />
        <result column="nameCard.tmplatSeCodeNm"           property="nameCard.tmplatSeCodeNm"          />
        <result column="nameCard.emplyrId"           property="nameCard.emplyrId"          />
    </resultMap>
    <select id="getDetail" parameterType="com.intermorph.cmmn.sample.vo.condition.LBSampleCondtion" resultMap="detailResultMap">
/* SampleMapper.getDetail */

SELECT 
	CODE_ID AS "nameCard.searchCnd",
	CODE_ID_NM AS "nameCard.tmplatSeCodeNm",
	CODE_ID_DC  AS "nameCard.emplyrId"
FROM 
	COMTCCMMNCODE 
where 
	CODE_ID =#{srchCodeId}
    </select>



</mapper>